{
  "name": "core-systems",
  "version": "1.0.0",
  "description": "Discord Security Bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:esm": "node index.mjs",
    "dev": "nodemon index.js",
    "stop": "taskkill /F /IM node.exe",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf node_modules",
    "reinstall": "npm run clean && npm install",
    "update": "npm update",
    "debug": "node --inspect index.js",
    "watch": "nodemon --watch src --ext js,json",
    "deploy": "node deploy-commands.js",
    "backup": "node scripts/backup.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "restart": "pm2 restart all",
    "status": "pm2 status"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.18.0",
    "better-sqlite3": "^11.6.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "opusscript": "^0.0.8",
    "play-dl": "^1.9.7",
    "quick.db": "^9.1.7",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "pm2": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
